    "@babel/traverse": "^7.28.5",
    "@babel/types": "^7.28.5"
  },
  "peerDependencies": {
    "@babel/core": "^7.28.5"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@types/node": "^25.0.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
